{
    "name": "erwane/cakephp-libs",
    "description": "Erwane CakePHP 3.x Libs (aka Ecl)",
    "type": "library",
    "authors": [
        {
            "name": "Erwane Breton",
            "email": "erwane@phea.fr"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=5.6",
        "cakephp/cakephp": "~3.3"
    },
    "require-dev": {
        "phpunit/phpunit": "5.*"
    },
    "autoload": {
        "psr-4": {
