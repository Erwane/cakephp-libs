{
    "name": "erwane/cakephp-libs",
    "description": "Erwane CakePHP 3.x Libs (aka Ecl)",
    "type": "cakephp-plugin",
    "authors": [
        {
            "name": "Erwane Breton",
            "homepage": "https://erwane-breton.fr",
            "email": "erwane@phea.fr"
        }
    ],
    "support": {
        "issues": "https://github.com/Erwane/cakephp-libs/issues"
    },
    "minimum-stability": "stable",
    "require": {
        "php": ">=7",
        "cakephp/cakephp": "^3.5",
        "ezyang/htmlpurifier": "^4.10"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5"
    },
    "autoload": {
        "psr-4": {
            "Ecl\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ecl\\Test\\": "tests"
        }
    }
}
